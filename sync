#!/usr/bin/env fish

# general
rm -f *.html
rm -f *.css
rm -f *.org
cp -r $HOME/Dropbox\ \(Brown\)/lyt/sites/ohrg.org/*.html .
cp -r $HOME/Dropbox\ \(Brown\)/lyt/sites/ohrg.org/*.css .
cp -r $HOME/Dropbox\ \(Brown\)/lyt/sites/ohrg.org/*.pdf .

# images
rm -rf img/
mkdir -p img
cp -r $HOME/Dropbox\ \(Brown\)/lyt/sites/ohrg.org/img/* ./img/

# raw files
cp -r $HOME/Dropbox\ \(Brown\)/lyt/sites/ohrg.org/writing-setup.org .
